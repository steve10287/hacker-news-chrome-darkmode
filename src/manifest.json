{
  "name": "Hacker News Dark Mode",
  "description": "Simple HN Dark Mode by steveb.co",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
    "128": "logo.PNG"
  },
  "content_scripts": [
    {
      "matches": [
        "https://news.ycombinator.com/*"
      ],
      "js": [
        "contentScript.js"
      ],
      "run_at": "document_start"
    }
  ],
  "host_permissions": [
    "https://news.ycombinator.com/*"
  ]
}